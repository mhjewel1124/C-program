#include <stdio.h>
#include <math.h>
int main(){
    float a,b,c,d,r1,r2;
    printf("Enter a,b,c: ");
    scanf("%f%f%f",&a,&b,&c);
    d=b*b-4*a*c;
    if(d>0){
        r1=(-b+sqrt(d))/(2*a);
        r2=(-b-sqrt(d))/(2*a);
        printf("Roots: %.2f and %.2f",r1,r2);
    }else if(d==0)
        printf("Root: %.2f",-b/(2*a));
    else
        printf("Complex Roots");
    return 0;
}
