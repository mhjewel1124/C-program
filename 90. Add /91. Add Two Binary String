#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100];
    int x, y, carry=0, sum;
    char res[200];
    gets(a); 
    gets(b);

    int i = strlen(a)-1;
    int j = strlen(b)-1;
    int k = 0;

    while(i>=0 || j>=0 || carry){
        x = (i>=0)? a[i]-'0' : 0;
        y = (j>=0)? b[j]-'0' : 0;
        sum = x + y + carry;
        res[k++] = (sum%2)+'0';
        carry = sum/2;
        i--; j--;
    }

    res[k] = '\0';
    strrev(res);
    printf("%s", res);
}
