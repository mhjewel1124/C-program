#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    int i, j, n, found;
    gets(s);
    n = strlen(s);

    for(i=0;i<n;i++){
        found = 0;
        for(j=0;j<n;j++){
            if(i!=j && s[i]==s[j]) { found=1; break; }
        }
        if(!found){
            printf("%c", s[i]);
            return 0;
        }
    }
    printf("No unique char");
}
