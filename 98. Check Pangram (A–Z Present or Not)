#include <stdio.h>
#include <ctype.h>

int main(){
    char s[200];
    gets(s);

    int a[26]={0};
    for(int i=0; s[i]; i++){
        if(isalpha(s[i]))
            a[tolower(s[i])-'a'] = 1;
    }

    for(int i=0;i<26;i++)
        if(a[i]==0){
            printf("Not Pangram");
            return 0;
        }

    printf("Pangram");
}
