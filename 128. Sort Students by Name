#include <stdio.h>
#include <string.h>
struct S{ char name[20]; int age; };
int main(){
int n=5;
struct S s[n], t;
for(int i=0;i<n;i++) scanf("%s %d", s[i].name, &s[i].age);
for(int i=0;i<n;i++)
for(int j=i+1;j<n;j++)
if(strcmp(s[i].name, s[j].name) > 0){ t=s[i]; s[i]=s[j]; s[j]=t; }
for(int i=0;i<n;i++) printf("%s %d\n", s[i].name, s[i].age);
}
