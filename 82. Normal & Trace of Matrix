#include <stdio.h>
#include <math.h>

int main() {
    int a[10][10], n, sum=0, trace=0;
    scanf("%d",&n);

    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++){
            scanf("%d",&a[i][j]);
            sum += a[i][j]*a[i][j];
            if(i==j) trace += a[i][j];
        }

    printf("Normal = %.2f\n", sqrt(sum));
    printf("Trace = %d\n", trace);
}
