#include <stdio.h>
struct S{ int id, age; };
int main(){
int n=5;
struct S s[n], t;
for(int i=0;i<n;i++) scanf("%d %d", &s[i].id, &s[i].age);
for(int i=0;i<n;i++)
for(int j=i+1;j<n;j++)
if(s[i].age > s[j].age){ t=s[i]; s[i]=s[j]; s[j]=t; }
for(int i=0;i<n;i++) printf("%d %d\n", s[i].id, s[i].age);
}
