#include <stdio.h>
int main(){
    int a[10][10], n, s1=0, s2=0;
    scanf("%d",&n);

    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++){
            scanf("%d",&a[i][j]);
            if(i==j) s1+=a[i][j];
            if(i+j==n-1) s2+=a[i][j];
        }

    printf("Main Diagonal = %d\n", s1);
    printf("Secondary Diagonal = %d", s2);
}
