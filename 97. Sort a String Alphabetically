#include <stdio.h>
#include <string.h>

int main(){
    char s[100], t;
    int n;
    gets(s);
    n = strlen(s);

    for(int i=0;i<n;i++)
        for(int j=i+1;j<n;j++)
            if(s[i] > s[j]){
                t = s[i];
                s[i] = s[j];
                s[j] = t;
            }

    printf("%s", s);
}
